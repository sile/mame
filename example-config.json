{
  "keybindings": {
    "MAIN": {
      "C-c": {
        "action": "quit"
      },
      "C-g": {
        "action": "cancel"
      },
      "C-s": {
        "context": "SEARCH",
        "action": "search"
      },
      "C-f": {
        "context": "SEARCH",
        "action": [
          {
            "shell": {
              "command": "rustfmt",
              "args": [
                "--edition=2024"
              ]
            }
          }
        ]
      },
      "<UP>": {
        "label": "â†‘",
        "aliases": [
          "C-p",
          "C-<LEFT>"
        ],
        "hidden": false,
        "action": "up"
      }
    },
    "SEARCH": {
      "C-c": {
        "action": "quit"
      },
      "C-s": {
        "action": "goto-next-hit"
      },
      "C-g": {
        "context": "MAIN",
        "action": "cancel"
      }
    }
  }
}
